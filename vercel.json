{
    "version": 2,
    "framework": "laravel",
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "composer.json",
            "use": "@vercel/php"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "^/build/(.*)$",
            "dest": "/public/build/$1"
        },
        {
            "src": "^/favicon\\.ico$",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "^/robots\\.txt$",
            "dest": "/public/robots.txt"
        },
        {
            "src": "^/(?!api).*\\.(css|js|mjs|json|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|pdf|webp|map)$",
            "dest": "/public/$0"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}